                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
import ghpythonlib.treehelpers as th

def make_square(vals, x1, y1, z, el, iso):
    x2 = x1 + el
    y2 = y1 + el
    casenumber = 0
    for i in range(3,-1,-1):
        if vals[i] &gt; iso:
            casenumber += 1
        if i &gt; 0:
            casenumber = casenumber &lt;&lt; 1
    
    vertices = []
    offset = casenumber * 4
    for i in range(offset, offset+4):
        index = edge_table[i]
        if index &gt; -1:
            p = None
            if index==0:
                p = rg.Point3d(x1 + v(vals[0], vals[1], iso) * el, y1, z)
            elif index==1:
                p = rg.Point3d(x2, y1 + v(vals[1], vals[2], iso) * el, z)
            elif index==2:
                p = rg.Point3d(x1 + v(vals[3], vals[2], iso) * el, y2, z)
            elif index==3:
                p = rg.Point3d(x1, y1 + v(vals[0], vals[3], iso) * el, z)
            vertices.append(p)
    
    lines = []
    for i in range(0,len(vertices),2):
        l = rg.LineCurve(vertices[i], vertices[i+1])
        lines.append(l)
    
    return lines, el

edge_table = [
-1, -1, -1, -1, 0, 3, -1, -1, 1, 0, -1, -1, 1, 3, -1, -1, 2,
1, -1, -1, 0, 3, 2, 1, 2, 0, -1, -1, 2, 3, -1, -1, 3, 2, -1,
-1, 0, 2, -1, -1, 1, 0, 3, 2, 1, 2, -1, -1, 3, 1, -1, -1, 0,
1, -1, -1, 3, 0, -1, -1, -1, -1, -1, -1]

def v(v1, v2, iso):
    if abs(v2-v1) &lt; 0.000001:
        return 0
    
    return (iso - v1) / (v2 - v1)

def dispatchByHeight(c, lh, cl):
    p = c.PointAtNormalizedLength(0)
    for i,h in enumerate(lh):
        if p.Z == h:
            cl[i].append(c)

if __name__=='__main__':
    if not ot is None:
        curves = []
        layer_heights = []
        
        for l in ot.leafs:
            e = l.edge
            dv = []
            x = l.pos.X
            y = l.pos.Y
            z = l.pos.Z
            if not z in layer_heights:
                layer_heights.append(z)
            dv.append(ot.distobj.get_distance(x-e/2,y-e/2,z))
            dv.append(ot.distobj.get_distance(x+e/2,y-e/2,z))
            dv.append(ot.distobj.get_distance(x+e/2,y+e/2,z))
            dv.append(ot.distobj.get_distance(x-e/2,y+e/2,z))
            lines,diam = make_square(dv,x-e/2,y-e/2,z,e,0)
            curves.extend(lines)
        curves = rg.Curve.JoinCurves(curves)
        
        layer_heights = list(set(layer_heights))
        layer_heights.sort()
        curves_layers = [[] for i in range(len(layer_heights))]
        
        for i,curve in enumerate(curves):
            if not curve.IsClosed:
                if not len(rg.PolylineCurve.DuplicateSegments(curve)) &lt;= 2:
                    startPt =rg.Curve.PointAtNormalizedLength(curve,0)
                    endPt = rg.Curve.PointAtNormalizedLength(curve,1)
                    if endPt.DistanceTo(startPt) &lt;= tolerance:
                        pts = []
                        segments = curve.DuplicateSegments()
                        for segment in segments:
                            pts.append(segment.PointAtNormalizedLength(0))
                            pts.append(segment.PointAtNormalizedLength(1))
                        pts.append(endPt)
                        pts.append(startPt)
                        nCrv = rg.Polyline(pts)
                        dispatchByHeight(nCrv, layer_heights, curves_layers)
                    else:
                        dispatchByHeight(curve, layer_heights, curves_layers)
                else:
                    dispatchByHeight(curve, layer_heights, curves_layers)
            else:
                dispatchByHeight(curve, layer_heights, curves_layers)
        curves_layers = th.list_to_tree(curves_layers)</item>
