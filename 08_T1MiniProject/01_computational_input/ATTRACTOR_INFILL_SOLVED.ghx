                    <item name="CodeInput" type_name="gh_string" type_code="10">import math
import Rhino.Geometry as rg

def remap(val, min, max, minOut, maxOut):
    if val &gt; max:
        val = max
    elif val &lt; min:
        val = min
    span = max-min
    spanOut = maxOut - minOut
    thrust = (val-min)/abs(span)
    
    return minOut + (thrust*spanOut)

class Attractor(object):
    """
    this is the attractor class
    """
    def __init__(self,obj,dist,targ):
        self.obj = obj
        self.dist = dist
        self.targ = targ
    
    def getFactor(self,x,y,z):
        f = 1
        newPt = rg.Point3d(x,y,z)
        if type(self.obj) is list:
            distances = []
            factors = []
            for o in self.obj:
                if type(o) is rg.Point3d:
                    d = rg.Point3d.DistanceTo(o, newPt)
                    distances.append(d)
                    f = remap(d, self.dist[0], self.dist[1], self.targ[0], self.targ[1])
                    factors.append(f)
                elif type(o) is rg.Plane:
                    projectedPt = rg.Plane.ClosestPoint(o, newPt)
                    d = rg.Point3d.DistanceTo(projectedPt, newPt)
                    distances.append(d)
                    f = remap(d, self.dist[0], self.dist[1], self.targ[0], self.targ[1])
                    factors.append(f)
            
            m = min(distances)
            for d,newf in zip(distances, factors):
                if d == m:
                    f = newf
        else:
            if type(self.obj) is rg.Point3d:
                d = rg.Point3d.DistanceTo(self.obj, newPt)
                f = remap(d, self.dist[0], self.dist[1], self.targ[0], self.targ[1])
            elif type(self.obj) is rg.Plane:
                projectedPt = rg.Plane.ClosestPoint(self.obj, newPt)
                d = rg.Point3d.DistanceTo(projectedPt, newPt)
                f = remap(d, self.dist[0], self.dist[1], self.targ[0], self.targ[1])
        
        return f

class TPMS(object):
    def __init__(self, aObj, dist, targ, w=10.0):
        self.wl = w
        self.fact = self.wl/(math.pi)
        self.attractor = Attractor(aObj, dist, targ)
        self.dist = dist
        self.targ = targ
    
    def get_distance(self,x,y,z):
        f = self.attractor.getFactor(x,y,z)
#        px = (x*f)/self.fact
#        py = (y*f)/self.fact
#        pz = (z*f)/self.fact
        px = x/f
        py = y/f
        pz = z/self.fact
        return self.get_value(px,py,pz,f)

    def get_value(self,x,y,z):
        return 0

class Gyroid(TPMS):
    def get_value(self,px,py,pz,f):
#        return math.sin(px)*math.cos(py) + math.sin(py)*math.cos(pz) + math.sin(pz)*math.cos(px) + f
        return math.sin(px)*math.cos(py) + math.sin(py)*math.cos(pz) + math.sin(pz)*math.cos(px)

class SchwartzP(TPMS):
    def get_value(self,px,py,pz,f):
#        return math.cos(px)+math.cos(py)+math.cos(pz) + f
        return math.cos(px) + math.cos(py) + math.cos(pz)

class Diamond(TPMS):
    def get_value(self,px,py,pz,f):
        return (
            math.sin(px) * math.sin(py) * math.sin(pz) +
            math.sin(px) * math.cos(py) * math.cos(pz) +
            math.cos(px) * math.sin(py) * math.cos(pz) +
            math.cos(px) * math.cos(py) * math.sin(pz))

class FischerKoch(TPMS):
    def get_value(self,px,py,pz,f):
        return (
            math.cos(2*px) * math.sin(py) * math.cos(pz) +
            math.cos(2*py) * math.sin(pz) * math.cos(px) +
            math.cos(2*pz) * math.sin(px) * math.cos(py))

if __name__=="__main__":
