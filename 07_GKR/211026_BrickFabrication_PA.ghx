import simple_ur_script as ur
import simple_comm as c

import math as m


ROBOT_IP = "192.168.10.10"
global ROBOT_IP

SAFE_ROBOT_VEL= 0.1
global SAFE_ROBOT_SPEED

SAFE_ROBOT_ACC = 0.05
global SAFE_ROBOT_ACCEL

IO = 0
global IO


def tcp(script):
    script += ur.set_tcp_by_angles(
        0.0, 0.0, 74.0, m.radians(0.0), m.radians(180.0), m.radians(0))
    return script


def set_robot_base_plane_from_pts():

    pt_0 = rg.Point3d(327, 230, 0)  # base plane origin
    # point on base plane positive x direction
    pt_1 = rg.Point3d(499, 230, 0)
    pt_2 = rg.Point3d(499, 499, 0)  # point on base plane positive xy

    robot_base = rg.Plane(pt_0, pt_1-pt_0, pt_2-pt_0)
    return robot_base

def rhino_to_robot_space(in_plane):
    plane = in_plane.Clone()
    _r_matrix = rg.Transform.PlaneToPlane(
        rg.Plane.WorldXY, set_robot_base_plane_from_pts())
    plane.Transform(_r_matrix)
    return plane


def pickup_brick(script, pick_up_plane):
    """Example of a method's documentation.

    Parameters
    ----------
    point_x : float
    Description of the first param
    point_y : float
    Description of the second param
    """
    planes = []
    
    safe_distance = 50

    safe_plane = pick_up_plane.Clone()
    safe_plane.Translate(rg.Vector3d(0, 0, safe_distance))

    script += ur.move_l(safe_plane, SAFE_ROBOT_ACC, SAFE_ROBOT_VEL)
    planes.append(safe_plane)

    script += ur.move_l(pick_up_plane, SAFE_ROBOT_ACC, SAFE_ROBOT_VEL)
    planes.append(pick_up_plane)

    script += ur.set_digital_out(IO, True)
    script += ur.sleep(1)

    script += ur.move_l(safe_plane, SAFE_ROBOT_ACC, SAFE_ROBOT_VEL)
    planes.append(safe_plane)

    return script, planes

def place_brick(script, plane):
    """
    this function gereates the robotic sequience for placing a brick
    Requires a plane wich describes the possition of the brick"""
    planes = []
    
    safe_distance = 50

    safe_plane = plane.Clone()
    safe_plane.Translate(rg.Vector3d(0, 0, safe_distance))

    script += ur.move_l(safe_plane, SAFE_ROBOT_ACC, SAFE_ROBOT_VEL)
    planes.append(safe_plane)

    script += ur.move_l(plane, SAFE_ROBOT_ACC, SAFE_ROBOT_VEL)
    planes.append(plane)

    script += ur.set_digital_out(IO, False)
    script += ur.sleep(1)

    script += ur.move_l(safe_plane, SAFE_ROBOT_ACC, SAFE_ROBOT_VEL)
    planes.append(safe_plane)

    return script, planes

def send(script):
    """
    this funktion sends the script to the robot."""

    script = c.concatenate_script(script)
    c.send_script(script, ROBOT_IP)
    return script


