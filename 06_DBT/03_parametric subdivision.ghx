                    <item name="CodeInput" type_name="gh_string" type_code="10">import sys

#change this path to point to where your local mola folder is saved
#for windows users switch "\" to "/"
path = "C:/Users/pj_30/OneDrive/Desktop/Github"
if path not in sys.path:
    sys.path.append(path)

import mola
from mola import module_rhino

# initional shape
my_octahedron = mola.construct_octahedron(10)

#first step
my_subdivided_octahedron = mola.Mesh()
for f in my_octahedron.faces:
  new_faces = mola.subdivide_face_extrude_tapered(f, 8, 0.4)
  new_faces[-1].group = 1
  my_subdivided_octahedron.faces.extend(new_faces)

my_octahedron = my_subdivided_octahedron

#second step
my_subdivided_octahedron = mola.Mesh()
for f in my_octahedron.faces:
  if f.group == 0:
    my_subdivided_octahedron.faces.append(f)
  elif f.group == 1:
    new_faces = mola.subdivide_face_extrude_tapered(f, 5, 0.4)
    new_faces[-1].group = 2
    my_subdivided_octahedron.faces.extend(new_faces)

my_octahedron = my_subdivided_octahedron

#third step
my_subdivided_octahedron = mola.Mesh()
for f in (my_octahedron.faces):
  if f.group == 1:
    new_faces = mola.subdivide_face_split_grid(f, 4, 1)
    new_faces[-1].group = 3
    new_faces[0].group = 3
    my_subdivided_octahedron.faces.append(new_faces[-1])
    my_subdivided_octahedron.faces.append(new_faces[0])
  elif f.group == 0:
    new_faces = mola.subdivide_face_split_grid(f, 4, 1)
    new_faces[-1].group = 4
    new_faces[0].group = 4
    my_subdivided_octahedron.faces.append(new_faces[-1])
    my_subdivided_octahedron.faces.append(new_faces[0])
  else:
    my_subdivided_octahedron.faces.append(f)
my_octahedron = my_subdivided_octahedron

#forth step
my_octahedron.update_topology()
my_octahedron = mola.mesh_offset(my_octahedron, -0.5)


#fifth step
for i in range(2):
    my_subdivided_octahedron = mola.Mesh()
    for f in my_octahedron.faces:
        if f.group == 4:
          new_faces = mola.subdivide_face_extrude_tapered(f, 1/(i+1.5), 0.4)
          my_subdivided_octahedron.faces.extend(new_faces)
        else:
          my_subdivided_octahedron.faces.append(f)

    my_octahedron = my_subdivided_octahedron

#color faces by group
values = [f.group for f in my_octahedron.faces]
scheme = [(1,0.5,0),(1,0,0.5),(0,0.5,1),(0,1,0.5)]
cmap = mola.color_map(values, scheme)

#sixth step
my_octahedron.update_topology()
my_octahedron = mola.subdivide_mesh_catmull(my_octahedron)
properties = my_octahedron.face_properties(mola.face_curvature)
cmap = mola.color_map(properties, scheme)

#color faces by a color map
mola.color_faces_by_map(my_octahedron.faces, cmap)


# seventh step
my_octahedron.update_topology()
my_octahedron = mola.subdivide_mesh_catmull(my_octahedron, properties )


#eight step
my_octahedron = mola.subdivide_mesh_catmull(my_octahedron)


a = module_rhino.display_mesh(my_octahedron)

