                    <item name="CodeInput" type_name="gh_string" type_code="10">import sys

#change this path to point to where your local mola folder is saved
#for windows users switch "\" to "/"
path = "C:/Users/pj_30/OneDrive/Desktop/Github"
if path not in sys.path:
    sys.path.append(path)

import mola
from mola import module_rhino

import math

# create a new empty mesh
m3 = mola.Mesh()
num_corners = 5
radius = 10

# add a ring of vertices using polar coordinates
step = math.pi * 2 / num_corners
for i in range(num_corners):
    angle = i * step
    x = radius * math.cos(angle)
    y = radius * math.sin(angle)
    #v = mola.Vertex(x, y, 0)
    #m3.vertices.append(v)
    m3.add_vertex(x, y)

# create a center vertex at the origin
cen = mola.Vertex(0,0,7)
m3.vertices.append(cen)

# add triangular faces
for i in range(num_corners):
    # walk along the ring
    # v1 is the current vertex, v2 the next
    # if v1 is the last, v2 is the first again
    # this is done with modulo
    # i goes from 0 to num_corners-1, if i+1 == num_corners -&gt; 0
    v0 = m3.vertices[i]
    vnext = m3.vertices[(i+1)%num_corners]
    f = mola.Face([v0, vnext, cen])
    m3.faces.append(f)

