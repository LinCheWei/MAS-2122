import mola
from mola import module_rhino

# construct an icosahedron and subdivide iteratively, using the rule 'extrudeToPointCenter'
# mesh=mola.construct_box(-4,-3,-3,4,3,3)
mesh = mola.construct_icosahedron(5)
numberOfIterations = 1

for i in range(numberOfIterations):
    newMesh=mola.Mesh()
    for face in mesh.faces:
        # the extrusion height is dependent on the current iteration index
        resultingfaces=mola.subdivide_face_extrude_to_point_center(face,1/(i+1))
        newMesh.faces.extend(resultingfaces)
    mesh=newMesh

for i in range(numberOfIterations):
    newMesh=mola.Mesh()
    for face in mesh.faces:
        # the extrusion height is dependent on the current iteration index
        resultingfaces=mola.subdivide_face_extrude_tapered(face,1/(i+1),0.2, doCap=False)
        newMesh.faces.extend(resultingfaces)
    mesh=newMesh 

for i,f in enumerate(mesh.faces):
    fa = mola.face_area(f)
    fp = mola.face_perimeter(f)
    f.color = (1,0,0,1)
    if i%2==0:
        f.color = (0,1,0,1)

