                    <item name="CodeInput" type_name="gh_string" type_code="10">import sys

#change this path to point to where your local mola folder is saved
#for windows users switch "\" to "/"
path = "C:/Users/pj_30/OneDrive/Desktop/Github"
if path not in sys.path:
    sys.path.append(path)

import mola
from mola import module_rhino

#construct a Box Mesh using the mola library's factory.
my_box = mola.construct_box(x1, y1, z1, x2, y2, z2)

a = module_rhino.display_mesh(my_box)

# print the number of faces of the mesh
print(len(my_box.faces))
print(len(my_box.vertices))

# print the number of vertices on a face of the mesh
print(len(my_box.faces[0].vertices))

# print the vertex positions
for v in my_box.vertices:
    print(repr(v))</item>
