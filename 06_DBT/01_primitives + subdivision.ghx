if path not in sys.path:
    sys.path.append(path)

import mola
from mola import module_rhino

my_torus = mola.construct_torus(ringRadius, tubeRadius, ringN, tubeN)
# color mesh faces by their angle to the z-axis

if criteria == 0:
    mola.color_faces_by_vertical_angle(my_torus.faces)
elif criteria ==1:
    mola.color_faces_by_curvature(my_torus.faces)
elif criteria ==2:
    mola.color_faces_by_area(my_torus.faces)
elif criteria ==3:
    mola.color_faces_by_perimeter(my_torus.faces)
elif criteria ==4:
    mola.color_faces_by_compactness(my_torus.faces)
elif criteria ==5:
    mola.color_faces_by_horizontal_angle(my_torus.faces)

a = module_rhino.display_mesh(my_torus)

#make a sphere for every vertex
spheres = []
for v in my_torus.vertices:
    radius = mola.math_map(v.z,-4,4,0.5,1)
    s = mola.construct_sphere(radius, v.x, v.y, v.z, 9, 9)
    temp = module_rhino.display_mesh(s)
